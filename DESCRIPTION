    MASS,
    Matrix,
    officer (>= 0.3.8),
    raster,
    readxl,
    rhandsontable,
    sf,
    shiny,
    shinycssloaders,
    shinydashboard,
    shinyjs,
    sm,
    sp,
    splancs,
    zoo
Suggests: 
    DBI,
    RSQLite
Encoding: UTF-8
